#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../.0.1.6@atool-test-mocha/bin/atool-test-mocha" "$@"
  ret=$?
else 
  node  "$basedir/../.0.1.6@atool-test-mocha/bin/atool-test-mocha" "$@"
  ret=$?
fi
exit $ret
